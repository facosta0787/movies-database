{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier", "json"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "no-unused-vars": [
      "error",
      {
        "args": "after-used"
      }
    ],
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off"
  }
}
